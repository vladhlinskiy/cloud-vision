{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Text Extractor Offline",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes": {
            "default": "auto-detect"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Source Path",
          "name": "sourcePath"
        },
        {
          "widget-type": "textbox",
          "label": "Destination Path",
          "name": "destinationPath"
        },
        {
          "widget-type": "select",
          "label": "Mime Type",
          "name": "mimeType",
          "widget-attributes": {
            "default": "application/pdf",
            "values": [
              "application/pdf",
              "image/tiff"
            ]
          }
        },
        {
          "widget-type": "number",
          "label": "Batch Size",
          "name": "batchSize",
          "widget-attributes": {
            "default": "20"
          }
        }
      ]
    },
